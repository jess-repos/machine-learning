getAns = function(numChoice){
dataList = c()
solution = function(num){
if(num == 1) {return(dataList)}
if(num %% 2 == 1){
ans = num * 3 + 1
}else if(num %% 2 == 0){
ans = num / 2
}
dataList = c(dataList, ans)
return(c(dataList, solution(dataList[length(dataList)])))
}
return = solution(numChoice)
}
